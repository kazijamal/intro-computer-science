;; Find the most significant digit of a nonegative integer.
(define msd
  (Î» (n)
    (if (< n 10)
        n
        (msd (quotient n 10)))))

(check-expect (msd 5) 5)
(check-expect (msd 38) 3)
(check-expect (msd 123456789) 1)

